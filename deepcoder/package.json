{
  "name": "deepcoder-assistant",
  "displayName": "DeepCoder Assistant",
  "description": "DeepCoder is is your intelligent coding companion, seamlessly integrated into VS Code and Cursor. Powered by advanced AI, it provides real-time code suggestions, debugging help, and instant answers to your programming questionsâ€”all without leaving your editor. Whether you're writing a new feature, debugging complex logic, or learning a new framework, CodeAI Assistant is here to supercharge your productivity.",
  "version": "1.0.3",
  "publisher": "AbdulhamidSonaike1",
  "icon": "deepcoder.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ham12-3/deep_seek_extension.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ai-assistant.openChat",
    "onView:deepcoder.sidebar"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch"
  },
  "devDependencies": {
    "esbuild": "^0.12.15"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deepcoderChatContainer",
          "title": "DeepCoder",
          "icon": "deepcoder.svg"
        }
      ]
    },
    "views": {
      "deepcoderChatContainer": [
        {
          "type": "webview",
          "id": "deepcoder.sidebar",
          "name": "DeepCoder Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "ai-assistant.openChat",
        "title": "Open AI Chat"
      }
    ]
  }
}
